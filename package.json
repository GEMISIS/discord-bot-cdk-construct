{
  "name": "discord-bot-cdk-construct",
  "version": "0.1.7",
  "description": "A quick CDK Construct for creating a serverless Discord bot in AWS!",
  "author": {
    "name": "Gerald McAlister",
    "url": "https://geraldmcalister.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint src/*/*.ts",
    "fix-lint": "eslint src/*/*.ts --fix",
    "test": "jest",
    "cdk": "cdk"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.ts",
      "src/types.ts",
      "src/constructs",
      "src/functions/constants"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.104.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "aws-cdk": "1.104.0",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigateway": "^1.104.0",
    "@aws-cdk/aws-lambda": "^1.104.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.104.0",
    "@aws-cdk/aws-secretsmanager": "^1.104.0",
    "@aws-cdk/core": "1.104.0",
    "@types/aws-lambda": "^8.10.72",
    "esbuild": "^0.8.53",
    "source-map-support": "^0.5.16"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.853.0",
    "aws-sdk-mock": "^5.1.0",
    "axios": "^0.21.1",
    "requests": "^0.3.0",
    "source-map-support": "^0.5.16",
    "tweetnacl": "^0.14.5"
  }
}
